{
  "name": "@mobiscroll/angular",
  "version": "5.34.2",
  "description": "Angular UI library for progressive web and hybrid apps",
  "homepage": "https://mobiscroll.com/",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "mobile web",
    "hybrid apps",
    "javascript",
    "mobiscroll",
    "javascript library",
    "angular",
    "component library",
    "ui components",
    "ui kit",
    "ionic plugin"
  ],
  "author": {
    "name": "Mobiscroll",
    "url": "https://mobiscroll.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acidb/mobiscroll.git"
  },
  "bugs": {
    "url": "https://github.com/acidb/mobiscroll/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/esm5/mobiscroll.angular.min.js",
      "require": "./dist/js/mobiscroll.angular.min.js"
    },
    "./dist/css/*": "./dist/css/*",
    "./dist/js/*": "./dist/js/*"
  },
  "main": "dist/js/mobiscroll.angular.min.js",
  "module": "dist/esm5/mobiscroll.angular.min.js",
  "types": "dist/js/mobiscroll.angular.min.d.ts",
  "sideEffects": false,
  "style": "dist/css/mobiscroll.min.css",
  "files": [
    "dist/js/**",
    "dist/esm5/**",
    "dist/css/**"
  ],
  "peerDependencies": {
    "@angular/core": ">=13.0.0",
    "@angular/common": ">=13.0.0",
    "@angular/forms": ">=13.0.0"
  }
}
